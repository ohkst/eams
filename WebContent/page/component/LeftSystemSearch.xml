scwin.resetBtnFontWeight = function () {
    btnFilterALL.removeClass("on");
    btnFilterBA.removeClass("on");
    btnFilterAAScreen.removeClass("on");
    btnFilterAATR.removeClass("on");
    btnFilterDA.removeClass("on");
    btnFilterMember.removeClass("on");

}

scwin.btnFilterALL_onclick = function (e) {
    scwin.resetBtnFontWeight();
    btnFilterALL.addClass("on");

    if (searchboxOK.getValue() == "") {
        grpBA.hide();
        grpAAScreen.hide();
        grpAATR.hide();
        grpDA.hide();
        grpMember.hide();
    } else {

        if (unitTasks.getRowCount() > 0) {
            grpBA.show("");
        } else {
            grpBA.hide();
        }

        if (scrns.getRowCount() > 0) {
            grpAAScreen.show("");
        } else {
            grpAAScreen.hide();
        }

        if (svcs.getRowCount() > 0) {
            grpAATR.show("");
        } else {
            grpAATR.hide();
        }

        if (tables.getRowCount() > 0) {
            grpDA.show("");
        } else {
            grpDA.hide();
        }

        if (emps.getRowCount() > 0) {
            grpMember.show("");
        } else {
            grpMember.hide();
        }

    }
};

