scwin.onpageload = function () {
    var paramData = $p.getParameter("HomeSearch");
    if (paramData.name != "") {
        searchboxOK.setValue(paramData.name)
        scwin.requestSearch();
    } else {
        grpBA.hide();
        grpAAScreen.hide();
        grpAATR.hide();
        grpDA.hide();
        grpMember.hide();

        btnFilterALL.addClass("on");
    }
};

scwin.searchDetail = function (rowIndex, columnIndex, columnId, type) {
    // $p.log(rowIndex + columnId + columnId);

    var cellData = "";
    var id = "";
    if (type === 0) { // 업무
        cellData = unitTasks.getCellData(rowIndex, columnId);
        id = unitTasks.getCellData(rowIndex, "unitAfrsId");
    } else if (type === 1) { // 화면
        cellData = scrns.getCellData(rowIndex, columnId);
        id = scrns.getCellData(rowIndex, "arfsScrnId");
    } else if (type === 2) { // TR
        cellData = svcs.getCellData(rowIndex, columnId);
        id = svcs.getCellData(rowIndex, "svcId");
    } else if (type === 3) { // Table
        cellData = tables.getCellData(rowIndex, columnId);
        id = tables.getCellData(rowIndex, "tlbId");
    } else if (type === 4) { // 담당자
        cellData = emps.getCellData(rowIndex, columnId);
        id = emps.getCellData(rowIndex, "empId");
    }

    var rowJSON = {
        // "Personal Info": { "Name": "Lee", "Age": 50, "Married": false },
        // "Region": "Seoul",
        // "Carrier": true,
        "name": cellData,
        "id": id
    };

    // 새로 열릴 화면에 데이터를 전달 (dataObject를 통해 전달 가능)
    var obj = {
        "dataObject": {
            "type": "json",
            "name": "LeftSearch",
            "data": rowJSON
        }
    };

    scwin.setContentsSrc(type, obj);
};

scwin.setContentsSrc = async function (type, obj) {
    if (type === 0) {
        await $p.parent().wfmRightContents.setSrc("/page/component/search/SystemSearchResultBizDetail.xml", obj);
    } else if (type === 1) {
        await $p.parent().wfmRightContents.setSrc("/page/component/search/SystemSearchResultScreenDetail.xml", obj);
    } else if (type === 2) {
        await $p.parent().wfmRightContents.setSrc("/page/component/search/SystemSearchResultTRDetail.xml", obj);
    } else if (type === 3) {
        await $p.parent().wfmRightContents.setSrc("/page/component/search/SystemSearchResultDBDetail.xml", obj);
    } else if (type === 4) {
        await $p.parent().wfmRightContents.setSrc("/page/component/search/SystemSearchResultMemberDetail.xml", obj);
    }
};

scwin.requestSearch = function () {
    unitTasks.removeAll();
    scrns.removeAll();
    svcs.removeAll();
    tables.removeAll();
    emps.removeAll();

    grpBA.hide();
    grpAAScreen.hide();
    grpAATR.hide();
    grpDA.hide();
    grpMember.hide();

    if (searchboxOK.getValue() != "") {

        const baParam = {
            offset: 0,
            limit: 20,
            type: "PAGE",
            sortMethod: "NO",
            searchMethods: ["CONTAINS"],
            sortFields: ["unitAfrsName"],
            searchFields: ["unitAfrsName"],
            searchContent: searchboxOK.getValue()
        };

        $c.sbm.execute(sbmUnitTaskSearchList, baParam);


        const screenParam = {
            offset: 0,
            limit: 20,
            type: "PAGE",
            sortMethod: "NO",
            searchMethods: ["CONTAINS"],
            sortFields: ["scrnNo"],
            searchFields: ["scrnNo", "scrnName"],
            searchContent: searchboxOK.getValue()
        };

        $c.sbm.execute(sbmTaskScreenList, screenParam);


        const trParam = {
            offset: 0,
            limit: 20,
            type: "PAGE",
            sortMethod: "NO",
            searchMethods: ["CONTAINS"],
            sortFields: ["svcCd"],
            searchFields: ["svcCd", "korSvcName"],
            searchContent: searchboxOK.getValue()
        };

        $c.sbm.execute(sbmServiceList, trParam);


        const tablesParam = {
            offset: 0,
            limit: 20,
            type: "PAGE",
            sortMethod: "NO",
            searchMethods: ["CONTAINS"],
            sortFields: ["tblEngName"],
            searchFields: ["tblEngName"],
            searchContent: searchboxOK.getValue()
        };

        $c.sbm.execute(sbmTableList, tablesParam);


        const memberParam = {
            offset: 0,
            limit: 13,
            type: "PAGE",
            sortMethod: "NO",
            searchMethods: ["CONTAINS"],
            sortFields: ["empName"],
            searchFields: ["empName"],
            searchContent: searchboxOK.getValue()
        };

        $c.sbm.execute(sbmEmpSearchList, memberParam);
    }

    // 엔터치면 코드 적용
    // if (searchboxOK.getValue() == "") {
    //     grpBA.hide();
    //     grpAAScreen.hide();
    //     grpAATR.hide();
    //     grpDA.hide();
    //     grpMember.hide();
    // } else if ((e.charCode || e.keyCode) === 13) {
    //     // $p.parent().wfmRightContents.setSrc("/page/component/SystemSearchResultDetail.xml");
    // } else {
    // }

};

