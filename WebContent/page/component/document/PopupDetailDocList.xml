        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function () {
    var paramData = $p.getParameter("PopupData");

    const param = {
        id: paramData.id
    };

    if (paramData.type == "BA") {
        $c.sbm.execute(sbmBADocList, param);
    } else if (paramData.type == "AA") {
        $c.sbm.execute(sbmAADocList, param);
    } else if (paramData.type == "DA") {
        $c.sbm.execute(sbmDADocList, param);
    } else if (paramData.type == "TA") {
        $c.sbm.execute(sbmTADocList, param);
    }
};

scwin.grdDocList_oncellclick = function (rowIndex, columnIndex, columnId) {
    const id = docs.getCellData(rowIndex, "linkSysId");
    const name = docs.getCellData(rowIndex, "dcmtName");
    $p.parent().scwin.openDocDetailPopup(id, name);

    $p.closePopup();
};

scwin.sbmAADocList_submitdone = function (e) {
    displayDocs.setArray(docs.getAllArray());
};

scwin.sbmBADocList_submitdone = function (e) {
    displayDocs.setArray(docs.getAllArray());
};

scwin.sbmDADocList_submitdone = function (e) {
    displayDocs.setArray(docs.getAllArray());
};

scwin.sbmTADocList_submitdone = function (e) {
    displayDocs.setArray(docs.getAllArray());
};
]]></script>
