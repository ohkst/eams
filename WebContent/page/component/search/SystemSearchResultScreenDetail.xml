        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function () {
    const leftParam = $p.getParameter("LeftSearch");
    const popupParam = $p.getParameter("PopupData");

    let paramData = null;

    // 우선순위 판단: 팝업에서 들어왔을 경우 PopupData를 사용
    if (popupParam != null) {
        paramData = popupParam;
        // alert("PopupData로 진입");
    } else if (leftParam != null) {
        paramData = leftParam;
        // alert("LeftSearch로 진입");
    }

    if (paramData != null) {
        if (paramData.name) {
            mainTitle.setValue(paramData.name);  // name이 있을 때만 세팅
        }

        const param = {
            id: paramData.id
        };

        $c.sbm.execute(sbmTaskScreenDetail, param);
    } else {
        // alert("전달된 파라미터가 없습니다.");
    }
};

scwin.gridTRList_oncellclick = function (rowIndex, columnIndex, columnId) {
    const id = svcs.getCellData(rowIndex, "svcId");
    const name = svcs.getCellData(rowIndex, "korSvcName");

    var rowJSON = {
        "id": id,
        "name": name
    };

    var dataObject = {
        "type": "json",
        "name": "PopupData",
        "data": rowJSON
    };

    $c.util.openEamsPopup("/page/component/search/SystemSearchResultTRDetail.xml", "TR 상세", dataObject);
};

scwin.sbmTaskScreenDetail_submit = function (e) {

};

scwin.sbmTaskScreenDetail_submitdone = function (e) {
    const param = {
        id: scrnInfo.get("arfsScrnId")
    };
    
    $c.sbm.execute(sbmTaskScreenInfo, param);
};

scwin.sbmTaskScreenDetail_submiterror = function (e) {

};


scwin.sbmTaskScreenInfo_submitdone = function (e) {
    if (unitAfrss.getRowCount() > 0) {
        unitAfrs.setJSON(unitAfrss.getRowJSON(0));
    } else {
        alert("zero")
    }
};
]]></script>
